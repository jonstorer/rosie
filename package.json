{
  "name": "rosie",
  "version": "2.1.1",
  "description": "factory for building JavaScript objects, mostly useful for setting up test data. Inspired by factory_girl",
  "keywords": [
    "factory",
    "rosie",
    "test"
  ],
  "author": "Brandon Keepers <brandon@opensoul.org>",
  "contributors": [
    "Brian Donovan"
  ],
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "chai": "^4.3.8",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-mocha": "^10.4.1",
    "eslint-plugin-prettier": "^5.1.3",
    "mocha": "^10.3.0",
    "prettier": "^3.2.5",
    "prettier-check": "^2.0.0",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rosiejs/rosie.git"
  },
  "main": "src/rosie.js",
  "scripts": {
    "-pretest": "eslint src test/**/*.js && prettier-check src/rosie.js test/**/*.js README.md",
    "test": "yarn mocha --config test/mocha.conf.js",
    "test:inspect": "node inspect $(yarn bin mocha) --config test/mocha.conf.js",
    "watch": "yarn test --watch"
  },
  "files": [
    "src/rosie.js"
  ],
  "license": "MIT"
}
